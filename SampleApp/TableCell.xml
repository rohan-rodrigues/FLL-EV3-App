<UITableViewCell
    detailTextLabel.highlightedTextColor="white"
    detailTextLabel.text="{link}"
    detailTextLabel.textColor="#aaa"
    height="auto + 20"
    reuseIdentifier="standaloneCell"
    selectedBackgroundView.backgroundColor="colors.red"
    style="subtitle"
    textLabel.highlightedTextColor="white"
    textLabel.text="{videoName}">

    <UIButton
        highlightedImage="{whiteImage}"
        image="{image}"
        right="100% - 20"
        tintColor="colors.red"
        top="20"
    
    />
    
</UITableViewCell>
